    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: ingest.py ./examples/cw_eri.json",
            "type": "python",
            "request": "launch",
            "program": "ingest.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "./examples/cw_eri.json"
            ]
        },
        {
            "name": "Python: ingest.py -n ./examples/a_sys.json [defaults]",
            "type": "python",
            "request": "launch",
            "program": "ingest.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "-n", 
                "./examples/a_sys.json"
            ]
        },
        {
            "name": "Python: ingest.py ./examples/a_sys.json [use new-file]",
            "type": "python",
            "request": "launch",
            "program": "ingest.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "./examples/a_sys.json"
            ]
        }
    ]
